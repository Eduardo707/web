	<script src="js/cookieHandler.js"></script>
<script>


var token = getCookie('token');
    function register(){
        var url = "https://ptsii.herokuapp.com/pacients/new?access_token=" + token ;

var data = {};
data.nome = document.getElementById('nome').textContent;
data.email = document.getElementById('email').textContent;
data.medicEmail = document.getElementById('nome').textContent;
data.morada = document.getElementById('morada').textContent;
data.beneficiario = document.getElementById('beneficiario').textContent;
data.utente = document.getElementById('utente').textContent;

var json = JSON.stringify(data);

var xhr = new XMLHttpRequest();
xhr.open("POST", url, true);
xhr.setRequestHeader('Content-type','application/json; charset=utf-8');
xhr.onload = function () {
	var users = JSON.parse(xhr.responseText);
	if (xhr.readyState == 4 && xhr.status == "201") {
		console.table(users);
	} else {
		console.error(users);
	}
}
xhr.send(json);

    }
</script>